{
  "name": "ngx-swiper",
  "description": "Angular wrapper library for Swiper",
  "bugs": "https://github.com/zefoy/ngx-swiper-wrapper/issues",
  "version": "10.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve app",
    "build": "ng build lib",
    "deploy": "deploy-to-git",
    "prepare": "ng build lib --prod",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app"
  },
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zefoy/ngx-swiper-wrapper.git"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/ngx-swiper-wrapper.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --prod -- --base-href=ngx-swiper-wrapper --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "swiper": "^6.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular/cli": "^17.3.11",
    "@angular/compiler-cli": "^17.3.12",
    "@types/node": "^12.0.0",
    "@types/swiper": "^5.2.0",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.0",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.1.0",
    "mkdirp": "^1.0.0",
    "ng-packagr": "^17.3.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.6.0",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "terser": "^5.3.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.0.0",
    "typescript": "~5.4.5",
    "watch": "^1.0.0",
    "zone.js": "^0.14.10"
  }
}